<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <data>
        <record id="record_sequence_appointment" model="ir.sequence">
            <field name="name">sequence appointment</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">Appointment/</field>
            <field name="padding">10</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <record id="record_sequence_patient" model="ir.sequence">
            <field name="name">Hospital Patient</field>
            <field name="code">res.hospital.patient</field>
            <field name="prefix">Patient/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="record_sequence_message" model="ir.sequence">
            <field name="name">Message Type</field>
            <field name="code">res.message</field>
            <field name="prefix"></field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    

</odoo>
